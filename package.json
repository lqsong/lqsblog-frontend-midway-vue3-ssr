{
  "name": "lqsblog-frontend-midway-vue3-ssr",
  "version": "1.0.0",
  "description": "是一个PC端、WAP端自适应展示的一个前端网站前台，它基于midway-vue3-ssr实现。",
  "private": true,
  "dependencies": {
    "@midwayjs/bootstrap": "^3.12.5",
    "@midwayjs/core": "^3.12.3",
    "@midwayjs/decorator": "^3.12.3",
    "@midwayjs/info": "^3.12.10",
    "@midwayjs/koa": "^3.12.10",
    "@midwayjs/logger": "^2.19.3",
    "@midwayjs/static-file": "^3.12.10",
    "@midwayjs/validate": "^3.12.10",
    "axios": "^1.6.2",
    "cross-env": "^6.0.3",
    "element-plus": "^2.4.3",
    "koa-connect": "^2.1.0",
    "normalize.css": "^8.0.1",
    "nprogress": "^0.2.0",
    "only-allow": "^1.2.1",
    "pinia": "^2.1.7",
    "qs": "^6.11.2",
    "vite": "^2.9.16",
    "vue": "^3.3.8",
    "vue-router": "^4.2.5"
  },
  "devDependencies": {
    "@midwayjs/cli": "^1.3.22",
    "@midwayjs/mock": "^3.12.10",
    "@types/jest": "^26.0.24",
    "@types/koa": "^2.13.11",
    "@types/node": "14",
    "@vitejs/plugin-vue": "^2.3.4",
    "jest": "^26.6.3",
    "mwts": "^1.3.0",
    "rollup-plugin-analyzer": "^4.0.0",
    "rollup-plugin-external-globals": "^0.9.0",
    "sass": "^1.69.5",
    "svgo": "^3.0.4",
    "ts-jest": "^26.5.6",
    "typescript": "~4.6.4",
    "vite-plugin-compression": "^0.5.1",
    "vite-plugin-svg-icons": "^2.0.1",
    "vue-tsc": "^0.34.17"
  },
  "engines": {
    "node": ">=14.18.0"
  },
  "scripts": {
    "start": "cross-env NODE_ENV=production node ./bootstrap.js",
    "dev": "cross-env NODE_ENV=local midway-bin dev --ts",
    "build": "pnpm run clean && pnpm run build:midway && pnpm run build:web",
    "build:midway": "midway-bin build -c",
    "build:web": "pnpm run build:web:client && pnpm run build:web:server",
    "build:web:client": "vite --config web/vite.config.ts build --ssrManifest --outDir ../build/client",
    "build:web:server": "vite --config web/vite.config.ts build --ssr entry-server --outDir ../build/server",
    "clean": "midway-bin clean",
    "test": "midway-bin test --ts",
    "cov": "midway-bin cov --ts",
    "svgo": "svgo -f web/assets/iconsvg",
    "lint": "mwts check",
    "lint:fix": "mwts fix",
    "ci": "npm run cov",
    "preinstall": "only-allow pnpm"
  },
  "midway-bin-clean": [
    ".vscode/.tsbuildinfo",
    "build",
    "dist"
  ],
  "repository": {
    "type": "git",
    "url": ""
  },
  "keywords": [
    "vue3",
    "midway",
    "midwayjs",
    "element-plus",
    "antdv",
    "antd",
    "typescript",
    "pinia",
    "pnpm"
  ],
  "author": "LiQingSong<957698457@qq.com>",
  "license": "MIT"
}
